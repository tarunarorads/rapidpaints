{%- liquid
	if request.design_mode or search.terms == "submenu" or section.index == null
		assign lazyMn = false
	else
		assign lazyMn = section.settings.lazy_mn
	endif
-%}

{%- if lazyMn -%}
	{%- case block.type -%}
		{%- when 'mega_shop' -%}
			<hdt-lazy-submenu id="sub-{{ block.id }}" data-section="{{ section.id }}" class="hdt-sub-menu hdt-mega-menu hdt-mega-shop hdt-absolute" color-scheme="{{ block.settings.mega_menu_colors }}"> </hdt-lazy-submenu>
		{%- when 'mega_product' -%}
			<hdt-lazy-submenu id="sub-{{ block.id }}" data-section="{{ section.id }}" class="hdt-sub-menu hdt-mega-menu hdt-mega-products hdt-absolute" color-scheme="{{ block.settings.mega_menu_colors }}"> </hdt-lazy-submenu>
		{%- when 'mega_sale' -%}
			<hdt-lazy-submenu id="sub-{{ block.id }}" data-section="{{ section.id }}" class="hdt-sub-menu hdt-mega-menu hdt-mega-sale hdt-absolute" color-scheme="{{ block.settings.mega_menu_colors }}"> </hdt-lazy-submenu>
		{%- when 'mega_page' -%}
			<hdt-lazy-submenu id="sub-{{ block.id }}" data-section="{{ section.id }}" class="hdt-sub-menu hdt-mega-menu hdt-mega-page hdt-absolute" color-scheme="{{ block.settings.mega_menu_colors }}"> </hdt-lazy-submenu>
		{%- when 'html' -%}
			<hdt-lazy-submenu id="sub-{{ block.id }}" data-section="{{ section.id }}" class="hdt-sub-menu hdt-mega-menu hdt-mega-html hdt-absolute" color-scheme="{{ block.settings.mega_menu_colors }}"> </hdt-lazy-submenu>
		{%- when 'blog' -%}
			<hdt-lazy-submenu id="sub-{{ block.id }}" data-section="{{ section.id }}" class="hdt-sub-menu hdt-mega-menu hdt-mega-blogs hdt-absolute" color-scheme="{{ block.settings.mega_menu_colors }}"> </hdt-lazy-submenu>
	{%- endcase -%}
{%- else -%}
	{%- case block.type -%}
		{%- when 'mega_shop' -%}
			<div id="sub-{{ block.id }}" class="hdt-sub-menu hdt-mega-menu hdt-mega-shop hdt-absolute" color-scheme="{{ block.settings.mega_menu_colors }}">
				{%- if block.settings.collection_list == blank and link.links == blank -%}
					{{ 'sections.header.add_content' | t: item: "shop" }}
				{%- else -%}
					{%- liquid
						assign row_col = 1
						if link.links != blank and block.settings.collection_list != blank
							assign row_col = 2
							if link.links.size >= 3 and block.settings.collection_list.count >= 3
								assign row_col = 1
							endif
						endif

						assign current_width = settings.page_width | append: 'px'
						assign space_item = "30px"
						if row_col == 2
							assign current_width = current_width | divided_by: 2
						endif
					-%}
					{%- capture sizes -%}
						calc(({{ current_width }} -  ({{ space_item }} * ( {{ block.settings.collection_list.count }} - 1 ))) * 1 / {{ block.settings.collection_list.count }})
					{%- endcapture -%}
					<div class="hdt-container{% if block.settings.mega_width == "custom" %}-full{% endif %}">
						<div class="hdt-row-grid hdt-row-grid--modify hdt-grid-cols-{{ row_col }} hdt-justify-center">
							{%- if link.links != blank -%}
								<div class="hdt-menu-col">
									<div class="hdt-row-grid hdt-grid-cols-{{ link.links.size | at_most: 6 }} hdt-text-start">
										{%- for link in link.links -%}
											<div class="hdt-mega-item hdt-text-start">
												{%- if link.links != blank -%}
													<a id="hdt-mega-{{ link.handle }}" class="hdt-mega-heading hdt-inline-flex hdt-align-center hdt-justify-between hdt-relative" href="{{ link.url }}">
														{%- assign arr_title = link.title | split: '[' -%}
														{%- assign arr_label = arr_title[1] | split: ',' -%}
														<span class="hdt-menu-link-text hdt-block{% if arr_label.size > 0 %} hdt-relative hdt-title_size-{{ arr_title[0].size | at_most: 20 }} hdt-has-{{ arr_label.size | at_most: 2 }}label{% endif %}">
															<span class="hdt-block">{{ arr_title[0] }}
																{%- if arr_label.size > 0 -%}
																	<span class="hdt_label_menu hdt-inline-flex hdt-align-center hdt-justify-center hdt-text-xs">
																		{%- for label in arr_label limit: 2 -%}
																			{%- assign label_text = label | remove: 'label_' | remove: ','  | remove: ']' | strip -%}
																			<span class="hdt_label_{{ label_text | handleize }} hdt-flex hdt-align-center hdt-justify-center">{{ label_text }}</span>
																		{%- endfor -%}
																	</span>
																{%- endif -%}
															</span>
														</span>
													</a>
													<ul class="hdt-mega-menu-list">
														{%- for childlink in link.links -%}
															<li class="hdt-menu-item hdt-flex hdt-justify-center hdt-flex-col{% if childlink.links != blank %} hdt-has-children hdt-relative{% endif %}">
																<a id="hdt-menu-{{ childlink.handle }}" href="{{ childlink.url }}" class="hdt-menu-link hdt-flex hdt-align-center hdt-justify-between hdt-relative hdt-text-base hdt-font-normal {% if childlink.current and section.settings.active_link %} hdt-menu-link--active{% endif %}" {% if childlink.current %} aria-current="page" {% endif %}>
																	{%- assign arr_title = childlink.title | split: '[' -%}
																	{%- assign arr_label = arr_title[1] | split: ',' -%}
																	<span class="hdt-menu-link-text hdt-block{% if arr_label.size > 0 %} hdt-relative hdt-title_size-{{ arr_title[0].size | at_most: 20 }} hdt-has-{{ arr_label.size | at_most: 2 }}label{% endif %}">
																		<span class="hdt-block">{{ arr_title[0] }}
																			{%- if arr_label.size > 0 -%}
																				<span class="hdt_label_menu hdt-inline-flex hdt-align-center hdt-justify-center hdt-text-xs">
																					{%- for label in arr_label limit: 2 -%}
																						{%- assign label_text = label | remove: 'label_' | remove: ','  | remove: ']' | strip -%}
																						<span class="hdt_label_{{ label_text | handleize }} hdt-flex hdt-align-center hdt-justify-center">{{ label_text }}</span>
																					{%- endfor -%}
																				</span>
																			{%- endif -%}
																		</span>
																	</span>
																	{%- if childlink.links != blank -%}
																		<svg class="hdt-menu-item-arrow" xmlns="http://www.w3.org/2000/svg" width="10" height="7" viewBox="0 0 10 7" fill="none"><path d="M10 1.24243L5 6.24243L-2.18557e-07 1.24243L0.8875 0.354932L5 4.46743L9.1125 0.354931L10 1.24243Z" fill="currentColor"/></svg>
																	{%- endif -%}
																</a>
																{%- if childlink.links != blank -%}
																	<ul class="hdt-menu-list hdt-mega-sub-menu hdt-sub-menu-lever-2" role="list">
																		{%- for grandchildlink in childlink.links -%}
																			<li class="hdt-menu-item hdt-flex hdt-justify-center hdt-flex-col">
																				<a id="hdt-menu-{{ grandchildlink.handle }}" href="{{ grandchildlink.url }}" class="hdt-menu-link hdt-flex hdt-align-center hdt-justify-between hdt-relative hdt-text-base hdt-font-normal {% if childlink.current and section.settings.active_link %} hdt-menu-link--active{% endif %}" {% if grandchildlink.current %} aria-current="page" {% endif %}>
																					{%- assign arr_title = grandchildlink.title | split: '[' -%}
																					{%- assign arr_label = arr_title[1] | split: ',' -%}
																					<span class="hdt-menu-link-text hdt-block{% if arr_label.size > 0 %} hdt-relative hdt-title_size-{{ arr_title[0].size | at_most: 20 }} hdt-has-{{ arr_label.size | at_most: 2 }}label{% endif %}">
																						<span class="hdt-block">{{ arr_title[0] }}
																							{%- if arr_label.size > 0 -%}
																								<span class="hdt_label_menu hdt-inline-flex hdt-align-center hdt-justify-center hdt-text-xs">
																									{%- for label in arr_label limit: 2 -%}
																										{%- assign label_text = label | remove: 'label_' | remove: ','  | remove: ']' | strip -%}
																										<span class="hdt_label_{{ label_text | handleize }} hdt-flex hdt-align-center hdt-justify-center">{{ label_text }}</span>
																									{%- endfor -%}
																								</span>
																							{%- endif -%}
																						</span>
																					</span>
																				</a>
																			</li>
																		{%- endfor -%}
																	</ul>
																{%- endif -%}
															</li>
														{%- endfor -%}
													</ul>
												{%- else -%}
													<a id="hdt-mega-{{ link.handle }}" class="hdt-mega-heading hdt-flex hdt-align-center hdt-justify-between hdt-relative{% if link.current and section.settings.active_link %} hdt-menu-link--active{% endif %}" href="{{ link.url }}" {% if link.current %} aria-current="page" {% endif %}>
														{%- assign arr_title = link.title | split: '[' -%}
														{%- assign arr_label = arr_title[1] | split: ',' -%}
														<span class="hdt-menu-link-text hdt-block{% if arr_label.size > 0 %} hdt-relative hdt-title_size-{{ arr_title[0].size | at_most: 20 }} hdt-has-{{ arr_label.size | at_most: 2 }}label{% endif %}">
															<span class="hdt-block">{{ arr_title[0] }}
																{%- if arr_label.size > 0 -%}
																	<span class="hdt_label_menu hdt-inline-flex hdt-align-center hdt-justify-center hdt-text-xs">
																		{%- for label in arr_label limit: 2 -%}
																			{%- assign label_text = label | remove: 'label_' | remove: ','  | remove: ']' | strip -%}
																			<span class="hdt_label_{{ label_text | handleize }} hdt-flex hdt-align-center hdt-justify-center">{{ label_text }}</span>
																		{%- endfor -%}
																	</span>
																{%- endif -%}
															</span>
														</span>
													</a>
												{%- endif -%}
											</div>
										{%- endfor -%}
									</div>
								</div>
							{%- endif -%}
							{%- if block.settings.collection_list != blank -%}
								<link href="{{ 'hdt-collection-items.css' | asset_url }}" media="all">
								<div class="hdt-collections-col hdt-ratio--{{ block.settings.image_ratio }} {% if block.settings.image_size %} hdt-object-cover{% else %} hdt-object-contain{% endif %}" color-scheme="{{ block.settings.collection_colors }}">
									<div class="hdt-row-grid hdt-grid-cols-{{ block.settings.collection_list.count }} hdt-has-collection1" style="--content-space: 20px;">
										{%- for collection in block.settings.collection_list -%}
											<div class="hdt-mega-item hdt-text-start">
												{%- assign image = collection.metafields.theme.featured_image_collection.value | default: collection.image | default: collection.products.first.featured_image -%}
												<div class="hdt-collection-item hdt-collection__item hdt-mega-item-collection hdt-radius-f hdt-collection-style1">
													<div class="hdt-collection-inner hdt-relative hdt-oh hdt-grid">
														<div class="hdt-collection-img hdt-eff-img-zoom hdt-eff" style="--aspect-ratioapt: {{ image.aspect_ratio | default: 1.2 }};">
															<div class="hdt-ratio" style="--aspect-ratioapt: {{ image.aspect_ratio | default: 1 }};">
																{{ image | image_url: width: image.width | image_tag: loading: 'lazy', width: image.width, height: image.height, class: 'hdt-obj-eff', sizes: sizes, widths: '250, 345, 400, 500, 700, 800, 1000, 1400, 1600, 1800, 2000' }}
															</div>
															<a class="hdt-full-width-link"  aria-label="link" href="{{ collection.url }}"></a>
														</div>
														<div class="hdt-collection-content hdt-absolute hdt-place-self-end-center">
															<a class="hdt-collection-title hdt-text-lg hdt-font-medium" href="{{ collection.url }}">
																<div class="hdt-collection-wrapper hdt-flex hdt-flex-col hdt-justify-center hdt-align-center">
																	<span class="hdt-collection-title-text hdt-truncate">{{ collection.title }}</span>
																</div>
															</a>
														</div>
													</div>
												</div>
											</div>
										{%- endfor -%}
									</div>
								</div>
							{%- endif -%}
						</div>
					</div>
				{%- endif -%}
			</div>
		{%- when 'mega_blog' -%}
			<div id="sub-{{ block.id }}" class="hdt-sub-menu hdt-mega-menu hdt-mega-blogs hdt-absolute" color-scheme="{{ block.settings.mega_menu_colors }}">
				{%- if block.settings.blog == blank and link.links == blank -%}
					{{ 'sections.header.add_content' | t: item: "blog" }}
				{%- else -%}
					{%- liquid
						assign row_col = 1
						if link.links != blank and block.settings.blog != blank
							assign row_col = 2
							if link.links.size >= 3 and block.settings.blog.count >= 3
								assign row_col = 1
							endif
						endif

						assign current_width = settings.page_width | append: 'px'
						assign space_item = "30px"
						if row_col == 2
							assign current_width = current_width | divided_by: 2
						endif
					-%}
					{%- capture sizes -%}
						calc(({{ current_width }} -  ({{ space_item }} * ( {{ block.settings.blog.count }} - 1 ))) * 1 / {{ block.settings.blog.count }})
					{%- endcapture -%}
					<div class="hdt-container{% if block.settings.mega_width == "custom" %}-full{% endif %}">
						<div class="hdt-flex hdt-header-gap-30">
							{%- if link.links != blank -%}
								<div class="hdt-menu-col {% if block.settings.blog != blank %}hdt-custom-col-4{% else %}hdt-custom-col-12{% endif %}">
									{%- for link in link.links -%}
										<div class="hdt-mega-item hdt-text-start">
											{%- if link.links != blank -%}
												<a id="hdt-mega-{{ link.handle }}" class="hdt-mega-heading hdt-inline-flex hdt-align-center hdt-justify-between hdt-relative" href="{{ link.url }}">
													{%- assign arr_title = link.title | split: '[' -%}
													{%- assign arr_label = arr_title[1] | split: ',' -%}
													<span class="hdt-menu-link-text hdt-block{% if arr_label.size > 0 %} hdt-relative hdt-title_size-{{ arr_title[0].size | at_most: 20 }} hdt-has-{{ arr_label.size | at_most: 2 }}label{% endif %}">
														<span class="hdt-block hdt-line-clamp">{{ arr_title[0] }}</span>
														{%- if arr_label.size > 0 -%}
															<span class="hdt_label_menu hdt-inline-flex hdt-align-center hdt-justify-center hdt-text-xs">
																{%- for label in arr_label limit: 2 -%}
																	{%- assign label_text = label | remove: 'label_' | remove: ','  | remove: ']' | strip -%}
																	<span class="hdt_label_{{ label_text | handleize }} hdt-flex hdt-align-center hdt-justify-center">{{ label_text }}</span>
																{%- endfor -%}
															</span>
														{%- endif -%}
													</span>
												</a>
												<ul class="hdt-mega-menu-list">
													{%- for childlink in link.links -%}
														<li class="hdt-menu-item hdt-flex hdt-justify-center hdt-flex-col{% if childlink.links != blank %} hdt-has-children hdt-relative{% endif %}">
															<a id="hdt-menu-{{ childlink.handle }}" href="{{ childlink.url }}" class="hdt-menu-link hdt-flex hdt-align-center hdt-justify-between hdt-relative hdt-text-base hdt-font-normal {% if childlink.current and section.settings.active_link %} hdt-menu-link--active{% endif %}" {% if childlink.current %} aria-current="page" {% endif %}>
																{%- assign arr_title = childlink.title | split: '[' -%}
																{%- assign arr_label = arr_title[1] | split: ',' -%}
																<span class="hdt-menu-link-text hdt-block{% if arr_label.size > 0 %} hdt-relative hdt-title_size-{{ arr_title[0].size | at_most: 20 }} hdt-has-{{ arr_label.size | at_most: 2 }}label{% endif %}">
																	<span class="hdt-block hdt-line-clamp">{{ arr_title[0] }}</span>
																	{%- if arr_label.size > 0 -%}
																		<span class="hdt_label_menu hdt-inline-flex hdt-align-center hdt-justify-center hdt-text-xs">
																			{%- for label in arr_label limit: 2 -%}
																				{%- assign label_text = label | remove: 'label_' | remove: ','  | remove: ']' | strip -%}
																				<span class="hdt_label_{{ label_text | handleize }} hdt-flex hdt-align-center hdt-justify-center">{{ label_text }}</span>
																			{%- endfor -%}
																		</span>
																	{%- endif -%}
																</span>
																{%- if childlink.links != blank -%}
																	<svg class="hdt-menu-item-arrow" xmlns="http://www.w3.org/2000/svg" width="10" height="7" viewBox="0 0 10 7" fill="none"><path d="M10 1.24243L5 6.24243L-2.18557e-07 1.24243L0.8875 0.354932L5 4.46743L9.1125 0.354931L10 1.24243Z" fill="currentColor"/></svg>
																{%- endif -%}
															</a>
															{%- if childlink.links != blank -%}
																<ul class="hdt-menu-list hdt-mega-sub-menu hdt-sub-menu-lever-2" role="list">
																	{%- for grandchildlink in childlink.links -%}
																		<li class="hdt-menu-item hdt-flex hdt-justify-center hdt-flex-col">
																			<a id="hdt-menu-{{ grandchildlink.handle }}" href="{{ grandchildlink.url }}" class="hdt-menu-link hdt-flex hdt-align-center hdt-justify-between hdt-relative hdt-text-base hdt-font-normal {% if childlink.current and section.settings.active_link %} hdt-menu-link--active{% endif %}" {% if grandchildlink.current %} aria-current="page" {% endif %}>
																				{%- assign arr_title = grandchildlink.title | split: '[' -%}
																				{%- assign arr_label = arr_title[1] | split: ',' -%}
																				<span class="hdt-menu-link-text hdt-block{% if arr_label.size > 0 %} hdt-relative hdt-title_size-{{ arr_title[0].size | at_most: 20 }} hdt-has-{{ arr_label.size | at_most: 2 }}label{% endif %}">
																					<span class="hdt-block hdt-line-clamp">{{ arr_title[0] }}</span>
																					{%- if arr_label.size > 0 -%}
																						<span class="hdt_label_menu hdt-inline-flex hdt-align-center hdt-justify-center hdt-text-xs">
																							{%- for label in arr_label limit: 2 -%}
																								{%- assign label_text = label | remove: 'label_' | remove: ','  | remove: ']' | strip -%}
																								<span class="hdt_label_{{ label_text | handleize }} hdt-flex hdt-align-center hdt-justify-center">{{ label_text }}</span>
																							{%- endfor -%}
																						</span>
																					{%- endif -%}
