                          endfor
                        when '10'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'right'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product10', card_product: product, section_id: section.id, class: "hdt-pr-style10", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                      endcase
                    endif
                  -%}
                </hdt-reval-items>
                {%- if paginate.pages > 1 -%}
                  {%- render 'pagination', paginate: paginate, anchor: '', section_id: section.id , pagination_type: section.settings.pagination_type, append_id: products_id, paginate_pos: section.settings.paginate_pos, dis_pagination: section.settings.dis_pagination, lm_text: section.settings.btn_label -%}
                {%- endif -%}
              {%- endif -%}
            {%- endpaginate -%}
          {%- endif -%}
        </div>
        {%- if has_sidebar -%}
          <aside class="hdt-main-sidebar hdt-col hdt-sidebar-{{ section.settings.sidebar_size }} {% if section.settings.sidebar_position == 'left' %} lg:hdt-order-first hdt-left-0 hdt-main-sidebar-left{% else %}hdt-right-0 hdt-main-sidebar-right{% endif %}"  {%- if section.settings.colors_by_section_sb %} color-scheme="{{ section.settings.color_scheme_sb }}" style="--background-opacity: {{ section.settings.background_opacity_sb }};"{%- endif %}>
            <div class="hdt-main-sidebar-inner hdt-current-scrollbar">
              <button aria-controls="hdt-shop-sidebar-drawer" class="lg:hdt-hidden hdt-flex hdt-align-center hdt-sidebar-btn hdt-fixed {% if section.settings.sidebar_position == 'left' %} hdt-left-0{% else %}hdt-right-0{% endif %}">
                <span class="hdt-btn-icon">
                  <svg width="24px" height="24px" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M21 3C21.5523 3 22 3.44772 22 4V20C22 20.5523 21.5523 21 21 21H3C2.44772 21 2 20.5523 2 20V4C2 3.44772 2.44772 3 3 3H21ZM7 5H4V19H7V5ZM20 5H9V19H20V5Z"></path></svg>
                </span>
                <span class="hdt-btn-text">{{ 'onboarding.open_sidebar' | t }}</span>
              </button>
              <hdt-drawer class="hdt-drawer hdt-sidebar-drawer hdt-contents" {%- if section.settings.colors_by_section_sb %} color-scheme="{{ section.settings.color_scheme_sb }}" style="--background-opacity: {{ section.settings.background_opacity_sb }};"{%- endif %}>
                <dialog id="hdt-shop-sidebar-drawer" class="hdt-dialog hdt-dialog-drawer" scroll-lock pos="{{ section.settings.sidebar_position }}">
                  <div class="hdt-dialog-drawer__header lg:hdt-hidden">
                    <h6 class="hdt-flex hdt-align-center hdt-sidebar-title hdt-filter-title hdt-text-lg hdt-uppercase hdt-font-semibold">
                      <span>{{- 'products.sidebar.label' | t -}}</span>
                    </h6>
                    <button ref="closeButton" class="hdt-s-text hdt-dialog-btn__close" aria-controls="hdt-shop-sidebar-drawer" aria-label="{{ 'accessibility.close' | t | escape }}">{%- render 'icon' with 'close' -%}</button>
                  </div>
                  <div class="hdt-dialog-drawer__inner hdt-current-scrollbar">
                    <div class="hdt-main-sidebar-inner">
                      {% render 'sidebar-collection' %}
                    </div>
                  </div>
                </dialog>
              </hdt-drawer>
            </div>
          </aside>
        {%- endif -%}
      </div>
    </div>
  </div>
  {%- if section.settings.description_position == 'after' -%}
    {{ collection_description }}
  {%- endif -%}
</div>

{%- style -%}{% if section.settings.section_layout == "full_width" %}#{{section.id}} .hdt-container-full{padding-inline-start:5px;padding-inline-end:5px}@media(min-width:768px){#{{section.id}} .hdt-container-full{padding-inline-start:15px;padding-inline-end:15px}}@media(min-width:1150px){#{{section.id}} .hdt-container-full{padding-inline-start:50px;padding-inline-end:50px}}{% endif %}{%- endstyle -%}
{% schema %}
{
