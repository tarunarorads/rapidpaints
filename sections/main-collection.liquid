{%- comment -%}{{ 'hdt-card-products.css' | asset_url | stylesheet_tag }}{%- endcomment -%}

{{ 'hdt-main-pages.css' | asset_url | stylesheet_tag }}
{{ 'facets.css' | asset_url | stylesheet_tag }}
{{ 'hdt-widget.css' | asset_url | stylesheet_tag }}
{%- liquid
  echo 'hdt-pagination.css' | asset_url | stylesheet_tag
  assign color_list = 'products.theme_settings.color' | t | downcase | strip | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq
  assign size_list = 'products.theme_settings.size' | t | downcase | strip | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq
  assign products_id = 'products-' | append: section.id
  assign arr_col_from_attr = cart.attributes.products_items_per_row | split: '.'
  assign col_mb = section.settings.col_mb | plus: 0
  assign col_tb = section.settings.col_tb | plus: 0
  assign col_dk = section.settings.col_dk | plus: 0
  assign col_mobile = arr_col_from_attr[0] | default: col_mb | plus: 0
  assign col_tablet = arr_col_from_attr[1] | default: col_tb | plus: 0
  assign col_tablet = col_tablet | at_most: col_dk | plus: 0
  assign col_desktop = arr_col_from_attr[2] | default: col_dk | plus: 0
  if section.blocks.size > 0
    assign has_sidebar = true
    case section.settings.sidebar_size
      when 'small'
        assign width_sidebar = '20%'
      when 'medium'
        assign width_sidebar = '25%'
      else
      assign width_sidebar = '33.3333333%'
    endcase
  else
    assign has_sidebar = false
  endif
  assign blocks_type = section.blocks | map: "type"
-%}
{%- liquid
  assign current_width = settings.page_width | append: 'px'
  assign current_width_tb = '100vw'
  assign current_width_mb = '100vw'
  if section.settings.section_layout == "full_width"
      assign current_width = '100vw - 100px'
      assign current_width_tb = '100vw - 20px'
      assign current_width_mb = '100vw - 10px'
  endif
  if section.settings.space_items == "x"
    assign space_item = settings.spacing_grid_horizontal
    assign space_item_tb = settings.spacing_grid_horizontal | divided_by: 2
    assign space_item_mb = settings.spacing_grid_horizontal | divided_by: 2
  else
    assign space_item = section.settings.space_items
    assign space_item_tb = space_item | times: 0.666 | round
    assign space_item_mb = space_item | times: 0.333 | round
  endif


  case section.settings.section_layout 
    when 'container'
      assign parent = 'hdt-container'
      assign child  = 'hdt-section-spacing hdt-s-gradient hdt-relative'
    when 'wrapper_container'
      assign parent = 'hdt-section-spacing hdt-s-gradient hdt-relative'
      assign child  = 'hdt-container'
    when 'full_width'
      assign parent = 'hdt-container-full hdt-section-spacing hdt-s-gradient hdt-relative'
      assign child  = ''
      assign sizes  = '100vw'
  endcase
-%}

{%- capture sizes -%}
  (min-width: 1150px) calc(({{ current_width }} -  {{ space_item }} * {{ section.settings.col_dk | minus: 1 }}) * {{ 1.00 | divided_by: section.settings.col_dk | round: 2 }}) {{- -}}
  , (min-width: 768px) calc( ({{ current_width_tb }} - {{ space_item_tb }} * {{ section.settings.col_tb | minus: 1 }} ) * {{ 1.00 | divided_by: section.settings.col_tb | round: 2 }})  {{- -}}
  , calc( ({{ current_width_mb }} - {{ space_item_mb }} * {{ section.settings.col_mb | minus: 1 }} ) * {{ 1.00 | divided_by: section.settings.col_mb | round: 2 }})
{%- endcapture -%}

{%- style -%}
  #shopify-section-{{ section.id }}{
    --section-pt: {{ section.settings.padding_top }}px;
    --section-pb: {{ section.settings.padding_bottom }}px; 
    --section-prate: {{ section.settings.padding_rate }};
  }
  #{{ section.id }} .hdt-row-grid {
    --spacing-x: {{ space_item_mb }}px;
    --spacing-y: {{ space_item }}px;
  } 
  @media(min-width: 768px){
    #{{ section.id }} .hdt-row-grid {
      --spacing-x: {{ space_item_tb }}px;
    } 
  }
  @media(min-width: 1150px){
    #{{ section.id }} .hdt-row-grid {
      --spacing-x: {{ space_item }}px;
    } 
  }
  {%- if has_sidebar -%}
    @media(min-width:1150px){
      .hdt-row-flex .hdt-shop-content {
        width: 100%;
      }
      .hdt-row-flex.lg\:hdt-flex-cols-2 .hdt-shop-content {
        width: calc(100% - {{ width_sidebar }} - var(--spacing-x)/2);
      }
      .hdt-row-flex.lg\:hdt-flex-cols-2 .hdt-main-sidebar {
        width:calc({{ width_sidebar }} - var(--spacing-x)/2);
      }
      
      /* .hdt-collection-products.lg\:hdt-grid-cols-4 .hdt-card-product:not(.hdt-pr-style2):not(.hdt-pr-style3):not(.hdt-pr-style4):not(.hdt-pr-style6):not(.hdt-pr-style7):not(.hdt-pr-style10):not(.hdt-pr-style11) .hdt-product-btns .hdt-pr_btn,
      .hdt-collection-products.lg\:hdt-grid-cols-4 .hdt-card-product .hdt-product-btns .hdt-pr_btn,.hdt-collection-products.lg\:hdt-grid-cols-4 .hdt-pr-style2 .hdt-card-product__media .hdt-product-btns .hdt-quantity-wrapp + .hdt-pr_btn {
        width:40px;
        min-width:40px;
        height:40px;
      } */
    }
    {%- if section.settings.sidebar_drawer == false -%}
      .hdt-row-flex .hdt-main-sidebar {
        width: calc({{ width_sidebar }} - var(--spacing-x) / 2);
      }
      .hdt-view-layout-switch button.hdt-view-grid5,
      .hdt-view-layout-switch button.hdt-view-grid6 {
        display:none
      }
    {%- endif -%}
  {%- endif -%}
{%- endstyle -%}
{%- capture sizes -%}
 {%- render 'sizes', enable_list: true, full_width: section.settings.section_layout == "full_width", col_mobile: col_mobile, col_tablet: col_tablet, col_desktop: col_desktop -%}
{%- endcapture -%}
{%- if section.settings.show_description and collection.description != blank -%}
  {%- capture collection_description -%}
    <div class="{{ parent }} hdt-main-collection-description" {%- if section.settings.colors_by_section %} color-scheme="{{ section.settings.color_scheme }}" style="--background-opacity: {{ section.settings.background_opacity }};"{%- endif %}>
      <div class="{{ child }}">
        <div class="hdt-rte">
          {{ collection.description }}
        </div>
      </div>
    </div>
  {%- endcapture -%}
{%- endif -%}
<div id="{{ section.id }}" main-content class="hdt-main-collection-content hdt-section-spacing">
  {%- if section.settings.description_position == 'before' -%}
    {{ collection_description }}
  {%- endif -%}
  <div class="{{ parent }}" {%- if section.settings.colors_by_section %} color-scheme="{{ section.settings.color_scheme }}" style="--background-opacity: {{ section.settings.background_opacity }};"{%- endif %}>
    <div class="{{ child }}">
      {%- if section.settings.section_background != blank -%}
        {% liquid
          assign image = section.settings.section_background
        %}
          {{ image | image_url: width: image.width | image_tag: width: image.width, height: image.height, sizes: sizes, widths: '200,300,400,500,600,700,800,900,1000,1100,1200,1300,1400,1500,1600,1700,1800,1900,2000', loading: 'lazy', class: 'hdt-section-background hdt-absolute hdt-top-0 hdt-left-0 hdt-w-full hdt-h-full hdt-pe-none hdt-object-cover hdt-object-center' }}
      {%- endif -%}
      {%- if collection.all_products_count > 0 -%}
      <div class="hdt-shop-control hdt-justify-between hdt-align-center hdt-row-flex hdt-flex-cols-3">
        {%- comment -%} Filter {%- endcomment -%}
          <div class="hdt-col hdt-control-filter">
            
            {%- if section.settings.enable_filtering -%}
              <button class="hdt-filter_btn hdt-inline-flex hdt-align-center {% if has_sidebar and blocks_type contains 'filter' -%} lg:hdt-hidden{%- endif -%}" aria-controls="hdt-filter_form">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" width="16" height="16"><path d="M324.4 64C339.6 64 352 76.37 352 91.63C352 98.32 349.6 104.8 345.2 109.8L240 230V423.6C240 437.1 229.1 448 215.6 448C210.3 448 205.2 446.3 200.9 443.1L124.7 385.6C116.7 379.5 112 370.1 112 360V230L6.836 109.8C2.429 104.8 0 98.32 0 91.63C0 76.37 12.37 64 27.63 64H324.4zM144 224V360L208 408.3V223.1C208 220.1 209.4 216.4 211.1 213.5L314.7 95.1H37.26L140 213.5C142.6 216.4 143.1 220.1 143.1 223.1L144 224zM496 400C504.8 400 512 407.2 512 416C512 424.8 504.8 432 496 432H336C327.2 432 320 424.8 320 416C320 407.2 327.2 400 336 400H496zM320 256C320 247.2 327.2 240 336 240H496C504.8 240 512 247.2 512 256C512 264.8 504.8 272 496 272H336C327.2 272 320 264.8 320 256zM496 80C504.8 80 512 87.16 512 96C512 104.8 504.8 112 496 112H400C391.2 112 384 104.8 384 96C384 87.16 391.2 80 400 80H496z"></path></svg>
                <span>{{ 'products.facets.filter_button' | t -}}</span>
              </button>
              {%- render 'facets', results: collection, section_id: section.id, enable_render_on_change: true -%}
            {%- endif -%}
          
          </div>
        {%- comment -%} endFilter {%- endcomment -%}
        <div class="hdt-col hdt-control-layout">
          {%- if section.settings.enable_num_cols_selector -%}
            {% liquid
              if section.settings.sidebar_drawer == false
                assign col_desktop = arr_col_from_attr[2] | default: col_dk | plus: 0 | at_most: 4
              endif
            %}
            {%- render 'layout-switch', results: collection, section_id: section.id, controls_id: products_id, col_mobile: col_mobile, col_tablet: col_tablet, col_desktop: col_desktop -%}
          {%- endif -%}
        </div>
        <div class="hdt-col hdt-control-sorting">
          {%- if section.settings.enable_sorting -%}
            {%- render 'sort-by-popover', results: collection, section_id: section.id -%}
          {%- endif -%}
        </div>
      </div>
      {%- endif -%}
      {%- render 'active-filters', results: collection, results_count: collection.products_count -%}
      <div class="{% if has_sidebar and section.settings.sidebar_drawer == false %} hdt-row-flex hdt-sidebar_drawer_{{ section.settings.sidebar_drawer }} hdt-flex-cols-1 lg:hdt-flex-cols-2{% else %} hdt-flex-cols-1{% endif %} ">
        <div class="hdt-shop-content hdt-col">
          {%- if collection.all_products_count == 0 -%}
            <div class="hdt-template hdt-template--empty hdt-text-center hdt-flex hdt-flex-col hdt-align-center hdt-justify-center">
              <p class="hdt-h5 hdt-s-text">{{ 'sections.collection_template.empty' | t -}}</p>
              <a class="hdt-btn hdt-btn hdt-oh hdt-inline-flex hdt-align-center hdt-justify-center hdt-relative hdt-btn-solid hdt-font-medium" href="{{ routes.all_products_collection_url }}" hdt-reveal="slide-in" timeline>
                {{ 'sections.collection_template.continue_shopping' | t }}
              </a>
            </div>
          {%- else -%}
            {%- paginate collection.products by section.settings.products_count -%}
              {%- if collection.products.size == 0 -%}
                {%- comment -%} no found {%- endcomment -%}
                <div class="hdt-template hdt-template--filtered hdt-template--empty hdt-text-center hdt-flex hdt-flex-col hdt-align-center hdt-justify-center">
                  <p class="hdt-h5 hdt-s-text">{{ 'products.facets.no_found' | t -}}</p>
                  {{ 'products.facets.use_fewer_filters_html' | t: link: collection.url, class: '' }}
                </div>
              {%- else -%}
                <hdt-reval-items reveal-on-scroll="true" id="{{ products_id }}" scroll-into-view class="hdt-collection-products hdt-collection-has-pr{{ section.settings.product_des }} hdt-pr-border_{{ section.settings.pr_border }} hdt-row-grid hdt-grid-cols-{{ col_mobile }} md:hdt-grid-cols-{{ col_tablet }} lg:hdt-grid-cols-{{ col_desktop }} hdt-ratio--{{ section.settings.image_ratio }}{% unless section.settings.image_size %} hdt-ratio--contain{% endunless %}">
                  {%- liquid
                    capture btn_wishlist_code
                      if settings.wishlist_mode != '0'
                        render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'right'
                      endif
                    endcapture
                    if col_mobile == 0 or col_tablet == 0 or col_desktop == 0
                      for product in collection.products
                        unless settings.remove_collection_url
                          assign product_url = product.url | within: collection
                        endunless
                        render 'card-product-list', card_product: product, class: "hdt-pr-list", section_id: section.id, sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                      endfor
                    else
                      case section.settings.product_des
                        when '1'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'right'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product1', card_product: product, class: "hdt-pr-style1", section_id: section.id, sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '2'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'top'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product2', card_product: product, section_id: section.id, class: "hdt-pr-style2", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '3'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'left'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product3', card_product: product, section_id: section.id, class: "hdt-pr-style3", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '4'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'top'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product4', card_product: product, section_id: section.id, class: "hdt-pr-style4", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '5'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'left'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product5', card_product: product, section_id: section.id, class: "hdt-pr-style5", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '6'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'left'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product6', card_product: product, section_id: section.id, class: "hdt-pr-style6", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '7'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'left'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product7', card_product: product, section_id: section.id, class: "hdt-pr-style7", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '8'
                          capture btn_wishlist_code
                            if settings.wishlist_mode != '0'
                              render 'btn-wishlist', class: 'hdt-card-product__btn-wishlist hdt-pr_btn', placement: 'left'
                            endif
                          endcapture
                          for product in collection.products
                            unless settings.remove_collection_url
                              assign product_url = product.url | within: collection
                            endunless
                            render 'card-product8', card_product: product, section_id: section.id, class: "hdt-pr-style8", sizes: sizes, image_ratio: section.settings.image_ratio, color_list: color_list, size_list: size_list, product_url: product_url, btn_wishlist_code: btn_wishlist_code, show_vendor: section.settings.show_vendor
                          endfor
                        when '9'
